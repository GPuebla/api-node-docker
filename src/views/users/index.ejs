<h1><%= title %></h1>

<table border="1" cellpadding="8" cellspacing="0">
  <thead>
    <tr>
      <th>Email</th>
      <th>Role</th>
    </tr>
  </thead>

  <tbody>
    <% if (users && users.length > 0) { %>
      <% users.forEach(user => { %>
        <tr>
          <td><%= user.email %></td>
          <td><%= user.role %></td>
        </tr>
      <% }) %>
    <% } else { %>
      <tr>
        <td colspan="2">No users found</td>
      </tr>
    <% } %>
  </tbody>
</table>
